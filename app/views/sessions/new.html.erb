<br><br><br>
<div class="col-8">
<h1>Login</h1>
<br>
<%= form_with(url: login_path, scope: :session, local: true) do |f| %>

      <%= f.label :email %>
      <%= f.email_field :email, class: 'form-control' %>
      <br>
      <%= f.label :password %>
      <%= f.password_field :password, class: 'form-control' %>
     <br>
      <%= f.submit "Log in", class: "btn btn-success" %>
    <% end %>
</div>

